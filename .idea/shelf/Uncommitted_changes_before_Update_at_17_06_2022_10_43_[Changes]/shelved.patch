+package service;
+
+import dao.AccountDAO;
+import entity.Account;
+import entity.Employee;
+import entity.Role;
+import model.AccountModel;
+import org.springframework.beans.factory.annotation.Autowired;
+import org.springframework.stereotype.Service;
+import utils.MyUtils;
+
+import java.util.List;
+
+@Service
+public class AccountService {
+    @Autowired
+    AccountDAO accountDAO;
+//    public List<AccountModel> getAccountModelListByEmployeeId()
+
+    public String addNewAccount(AccountModel accountModel) {
+        Account account = new Account();
+        Employee employee = new Employee();
+        Role role = new Role();
+        account.setEmail(accountModel.getEmail());
+        account.setPassword(MyUtils.passwordEncoder.encode(accountModel.getPassword()));
+        employee.setEmployeeId(accountModel.getEmployeeModel().getEmployeeId());
+        account.setEmployee(employee);
+        role.setRoleId(accountModel.getRoleId());
+        account.setRole(role);
+        account.setStatus(true);
+        if(checkExistsAccount(accountModel)) {
+            return "Email đã được đăng ký, vui lòng thử một email khác!";
+        }
+        if(accountDAO.addNew(account)) {
+           return "Thêm tài khoản thành công!" ;
+        }
+        return "Thêm tài khoản thất bại, vui lòng thử lại sau!";
+    }
+
+    public boolean checkExistsAccount(AccountModel accountModel) {
+        Account account = new Account();
+        account.setEmail(accountModel.getEmail());
+        return accountDAO.findById(account) != null;
+    }
+}
